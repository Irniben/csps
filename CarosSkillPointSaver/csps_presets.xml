<GuiXml>
	<Controls>
	
		<Control name="CSPSqsEditListEntry" mouseEnabled="true" virtual="true">
			<Dimensions y="28" />
			<OnMouseEnter>
				if self.tooltipFunction then self.tooltipFunction(self) end
			</OnMouseEnter>
			<OnMouseExit>
				ZO_Tooltips_HideTextTooltip()
				if self.mouseExitFunction then self:mouseExitFunction() end
			</OnMouseExit>
			<OnMouseUp>
				if button == 1 and self.data.leftClickFunction then 
					self.data.leftClickFunction() 
				elseif button == 2 and self.data.rightClickFunction then
					self.data.rightClickFunction()
				end
			</OnMouseUp>
			<Controls>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" color="A6D852" />
				<Label name="$(parent)Name" font="ZoFontGame">
					<Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)" offsetX="5"/>
					<Dimensions y="28" />
				</Label>
			</Controls>
		</Control>
		<Control name="CSPSOutfitLE" inherits="ZO_TreeEntry" virtual="true">
			<Dimensions  y="34" />
			<Controls>
				<Texture name="$(parent)Icon" mouseEnabled="true">
					<Dimensions x="30" y="30" />
				      <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetY="4" />
					  <OnMouseEnter>if self:GetParent().tooltipFunction then self:GetParent():tooltipFunction() end</OnMouseEnter>
					  <OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside, ctrl, alt, shift)</OnMouseUp>
					  <OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>	
				</Texture>	
				<Texture name="$(parent)Indicator">
					<Dimensions x="18" y="18" />
				    <Anchor point="BOTTOM" relativeTo="$(parent)Icon" relativePoint="BOTTOMRIGHT" offsetY="0" />
				</Texture>	
				<Button name="$(parent)BtnMinus">
					<Dimensions y="28" x="28" />
					<Anchor point="CENTER" relativePoint="CENTER" relativeTo="$(parent)Icon" constrains="Y" />
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="CSPSWindowScrollListScrollBar" constrains="X" offsetX="-4"/>
					<Textures normal="ESOUI/art/buttons/minus_up.dds" pressed="ESOUI/art/buttons/minus_down.dds" mouseOver="ESOUI/art/buttons/minus_over.dds" />
					<OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>	
				</Button>
				<Label name="$(parent)Text" font="ZoFontGame" mouseEnabled="true" linkEnabled="true">
					<Dimensions y="32" />
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Icon" offsetX="11"/>
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$(parent)BtnMinus" offsetX="-2" constrains="X"/>
					<OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside, ctrl, alt, shift)</OnMouseUp>
					<OnMouseEnter>if self:GetParent().tooltipFunction then self:GetParent():tooltipFunction() end</OnMouseEnter>
					<OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>	
				</Label>
			</Controls>
		</Control>
		<Control name="CSPSQSLE" inherits="ZO_TreeEntry" virtual="true">
			<Dimensions  y="34" />
			<Controls>
				<Texture name="$(parent)Marker" mouseEnabled="true">
					<Dimensions x="24" y="24" />
				      <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetY="4" />
					  <OnMouseEnter></OnMouseEnter>
					  <OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside, ctrl, alt, shift)</OnMouseUp>
					  <OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>	
				</Texture>	
				<Texture name="$(parent)Icon" mouseEnabled="true">
					<Dimensions x="30" y="30" />
				      <Anchor point="LEFT" relativeTo="$(parent)Marker" relativePoint="RIGHT"  offsetX="4"/>/>
					  <OnMouseEnter>if self:GetParent().tooltipFunction then self:GetParent():tooltipFunction() end</OnMouseEnter>
					  <OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside, ctrl, alt, shift)</OnMouseUp>
					  <OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>	
				</Texture>	
				<Texture name="$(parent)Indicator">
					<Dimensions x="18" y="18" />
				    <Anchor point="BOTTOM" relativeTo="$(parent)Icon" relativePoint="BOTTOMRIGHT" offsetY="0" />
				</Texture>	
				<Button name="$(parent)BtnMinus">
					<Dimensions y="28" x="28" />
					<Anchor point="CENTER" relativePoint="CENTER" relativeTo="$(parent)Icon" constrains="Y" />
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="CSPSWindowScrollListScrollBar" constrains="X" offsetX="-4"/>
					<Textures normal="ESOUI/art/buttons/minus_up.dds" pressed="ESOUI/art/buttons/minus_down.dds" mouseOver="ESOUI/art/buttons/minus_over.dds" />
					<OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>	
				</Button>
				<Label name="$(parent)Text" font="ZoFontGame" mouseEnabled="true" linkEnabled="true">
					<Dimensions y="32" />
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Icon" offsetX="11"/>
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$(parent)BtnMinus" offsetX="-2" constrains="X"/>
					<OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside, ctrl, alt, shift)</OnMouseUp>
					<OnMouseEnter>if self:GetParent().tooltipFunction then self:GetParent():tooltipFunction() end</OnMouseEnter>
					<OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>	
				</Label>
			</Controls>
		</Control>
		
		
		<Control name="CSPSPOISONLISTENTRY" mouseEnabled="true" virtual="true">
			<Dimensions y="28" />
			<OnMouseEnter>
				CSPS.poisonListMouseEnter(self, button, upInside)
			</OnMouseEnter>
			<OnMouseExit>
				CSPS.poisonListMouseExit(self, button, upInside)
			</OnMouseExit>
			<OnMouseUp>
				CSPS.poisonListMouseUp(self, button, upInside)
			</OnMouseUp>
			<Controls>
				<Texture name="$(parent)Selected" textureFile="EsoUI/Art/Miscellaneous/listItem_selectedHighlight.dds">
					<AnchorFill />
					<TextureCoords left="0" right="1" top="0" bottom=".625" />
				</Texture>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" color="A6D852" />
				<Label name="$(parent)Name" font="ZoFontGame">
					<Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)" offsetX="5"/>
					<Dimensions x="404" y="28" />
				</Label>
			</Controls>
		</Control>
		
		
		<Control name="CSPSComboLabelPair" virtual="true">
			<OnInitialized>
				self.label = GetControl(self, "Label")
				self.box = GetControl(self, "Box")
			</OnInitialized>
			<Dimensions y="28" />
			<Controls>
				<Label name="$(parent)Label" font="ZoFontGame" text="">
					<Dimensions  y="28" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
				</Label>
				<Control name="$(parent)Box" inherits="ZO_ComboBox">
					<Dimensions  y="25" />
					<Anchor point="LEFT" relativeTo="$(parent)Label" relativePoint="RIGHT"  offsetX="5" />
					<Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" constrains="X" />
				</Control>
			</Controls>
		</Control>
		
		
		<Control name="CSPSGearWindow" virtual="true" mouseEnabled="true" tier="1">
			<OnInitialized>
				CSPS.InitGearWindow(self)
			</OnInitialized>
			<Dimensions x="420" y="198" /> <!-- 8 + 28 + 8 + 28 + 8 + 28 + 8 + 28 + 8 = -->
			<Controls>
				<Backdrop name="$(parent)BG" centerColor="080808" edgeColor="424242"> 
					<Edge edgeSize="1" />
					<AnchorFill />
				</Backdrop>
				<Control name="$(parent)PoisonFields">
					<AnchorFill />
					<Controls>
						<Control name="$(grandparent)GeneralPoisons" inherits="CSPSComboLabelPair" tier="1" level="0">
							<Dimensions y="28" />
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)" relativePoint="TOPLEFT"  offsetX="8" offsetY="8" />
							<Anchor point="TOPRIGHT" relativeTo="$(grandparent)" relativePoint="TOP"  offsetX="-4" offsetY="8" />
						</Control>
						<Control name="$(grandparent)PoisonsSpecific" inherits="CSPSComboLabelPair" tier="1" level="0">
							<Dimensions y="28" />
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)" relativePoint="TOP"  offsetX="4" offsetY="8" />
							<Anchor point="TOPRIGHT" relativeTo="$(grandparent)" relativePoint="TOPRIGHT"  offsetX="-8" offsetY="8" />
						</Control>
						<Control name="$(parent)List" inherits="ZO_ScrollList">
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)GeneralPoisons" relativePoint="BOTTOMLEFT" offsetY="8" />
							<Anchor point="BOTTOMRIGHT" relativeTo="$(grandparent)" relativePoint="BOTTOMRIGHT" offsetX="-8" offsetY="-44" />
						</Control>
					</Controls>
				</Control>
				<Control name="$(parent)SetItemFields">
					<AnchorFill />
					<Controls>
						<Label name="$(grandparent)SetsLabel" font="ZoFontGame" text="Sets" tier="1" level="0">
							<Dimensions  y="24" />
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)" relativePoint="TOPLEFT"  offsetX="8" offsetY="8" />
						</Label>
						<Backdrop name="$(grandparent)Sets" tier="1" inherits="ZO_SingleLineEditBackdrop_Keyboard"> <!-- tier HIGH? -->
							<Dimensions  y="25" />					
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)SetsLabel" relativePoint="BOTTOMLEFT"  offsetY="5" />
							<Anchor point="RIGHT" relativeTo="$(grandparent)" relativePoint="RIGHT"  offsetX="-8" constrains="X" />
							<Controls>
								<EditBox name="$(parent)Edit" inherits="ZO_DefaultEditForBackdrop ZO_EditDefaultText">
									<OnInitialized>
										
									</OnInitialized>
								</EditBox>
							</Controls>

						</Backdrop>
					
						<Control name="$(grandparent)Type" inherits="CSPSComboLabelPair" tier="1" level="0">
							<Dimensions y="28" />
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)" relativePoint="TOPLEFT"  offsetX="5" offsetY="80" />
							<Anchor point="TOPRIGHT" relativeTo="$(grandparent)" relativePoint="TOP"  offsetX="-52" offsetY="80" />
						</Control>
						
						<Control name="$(grandparent)Enchantment" inherits="CSPSComboLabelPair" tier="1" level="0">
							<Dimensions y="28" />
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)" relativePoint="TOP"  offsetX="-42"  offsetY="80" />
							<Anchor point="TOPRIGHT" relativeTo="$(grandparent)" relativePoint="TOPRIGHT"  offsetX="-5" offsetY="80" />
						</Control>
						
						<Control name="$(grandparent)Quality" inherits="CSPSComboLabelPair" tier="1" level="0">
							<Dimensions y="28" />
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)Type" relativePoint="BOTTOMLEFT" offsetY="8" />
							<Anchor point="TOPRIGHT" relativeTo="$(grandparent)" relativePoint="TOP"  offsetX="-5"  constrains="X" />
						</Control>
						
						<Control name="$(grandparent)Trait" inherits="CSPSComboLabelPair" tier="1" level="0">
							<Dimensions y="28" />
							<Anchor point="TOPLEFT" relativeTo="$(grandparent)" relativePoint="TOP"  offsetX="5" constrains="X" />
							<Anchor point="TOPRIGHT" relativeTo="$(grandparent)Enchantment" relativePoint="BOTTOMRIGHT"  offsetY="8" />
						</Control>
					</Controls>	
				</Control>
				
				<Button name="$(parent)Cancel" inherits="ZO_DefaultButton" text="SI_DIALOG_CANCEL" tier="1" level="0">
					<Dimensions x="120" y="28" />
					<Anchor point="BOTTOMRIGHT" relativeTo="$(parent)" relativePoint="BOTTOMRIGHT" offsetX="-8" offsetY="-8" />
					<OnClicked>
						EVENT_MANAGER:UnregisterForEvent(CSPS.name.."GearWinHider", EVENT_GLOBAL_MOUSE_DOWN)
						self:GetParent():SetHidden(true)
					</OnClicked>
				</Button>
				
				<Button name="$(parent)Ok" inherits="ZO_DefaultButton" text="SI_DIALOG_CONFIRM" tier="1" level="0">
					<Dimensions x="120" y="28" />
					<Anchor point="RIGHT" relativeTo="$(parent)Cancel" relativePoint="LEFT" offsetX="-8" />
				</Button>
				
			</Controls>
		</Control>
		
		
		<Control name="CSPSLEGEAR" inherits="ZO_TreeEntry" virtual="true">
			<Dimensions  y="32" /> <!-- x: 388 + 134 = 522 to the right side of the last button?  78 = button + icon + offsets -->
			<OnInitialized>
				self.ctrIcon = self:GetNamedChild("Icon")
				self.ctrSetName = self:GetNamedChild("SetName")
				self.ctrTrait = self:GetNamedChild("Trait")
				self.ctrEnchantment = self:GetNamedChild("Enchantment")
				self.ctrBtnEdit = self:GetNamedChild("BtnEdit")
				self.ctrIndicator = self:GetNamedChild("Indicator")
				self.ctrIndicator2 = self:GetNamedChild("Indicator2")
			</OnInitialized>
			<OnMouseUp></OnMouseUp>
			<Controls>
				<Texture name="$(parent)Icon" mouseEnabled="true">
					<Dimensions x="30" y="30" />
				    <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetY="2" />
					<OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside)</OnMouseUp>
					<OnMouseEnter>if self:GetParent().tooltipFunction then self:GetParent().tooltipFunction(self) end</OnMouseEnter>
					<OnMouseExit>if self:GetParent().tooltipExitFunction then self:GetParent().tooltipExitFunction() end</OnMouseExit>	
					<OnReceiveDrag>if self:GetParent().receiveDragFunction then self:GetParent().receiveDragFunction() end</OnReceiveDrag>
				</Texture>	
				
				<Texture name="$(parent)Indicator">
					<Dimensions x="18" y="18" />
				    <Anchor point="BOTTOM" relativeTo="$(parent)Icon" relativePoint="BOTTOMRIGHT" offsetY="0" />
				</Texture>	
				
				<Texture name="$(parent)Indicator2" hidden="true" textureFile="esoui/art/tooltips/icon_lock.dds">
					<Dimensions x="12" y="12" />
				    <Anchor point="TOP" relativeTo="$(parent)Icon" relativePoint="TOPRIGHT" offsetY="0" />
				</Texture>	
				
				<!-- Just one Row:  (30 + 4) + 280 + 2 + 72 + 2 + 100 + (4 + 28) = 522 -->

				<Button name="$(parent)BtnEdit">
					<Dimensions x="28"  y="28" />
					<Anchor point="CENTER" relativePoint="CENTER" relativeTo="$(parent)Enchantment" constrains="Y"/>
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="CSPSWindowScrollListScrollBar" constrains="X" offsetX="-4"/>
					<Textures normal="ESOUI/art/buttons/edit_up.dds" pressed="ESOUI/art/buttons/edit_down.dds" mouseOver="ESOUI/art/buttons/edit_over.dds" />
				</Button>
				<Control name="$(parent)LblTraitEnchantmentContainer">
					<Anchor point="LEFT" relativePoint="CENTER" relativeTo="CSPSWindow" offsetX="21" constrains="X"/>
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$(parent)BtnEdit" offsetX="-2" />					
				</Control>
				
				<Label name="$(parent)SetName" font="$(MEDIUM_FONT)|$(KB_16)|soft-shadow-thin" mouseEnabled="true" verticalAlignment="CENTER" wrapMode="1">
					<Dimensions y="32" />
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Icon" offsetX="11" offsetY="0"/>
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$(parent)LblTraitEnchantmentContainer" offsetX="-2" constrains="X"/>
					<OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside)</OnMouseUp>
					<OnMouseEnter>if self:GetParent().tooltipFunction then self:GetParent().tooltipFunction(self) end</OnMouseEnter>
					<OnMouseExit>if self:GetParent().tooltipExitFunction then self:GetParent().tooltipExitFunction() end</OnMouseExit>
					<OnReceiveDrag>if self:GetParent().receiveDragFunction then self:GetParent().receiveDragFunction() end</OnReceiveDrag>
				</Label>
				<Label name="$(parent)Trait" font="$(MEDIUM_FONT)|$(KB_16)|soft-shadow-thin" mouseEnabled="true" verticalAlignment="CENTER" wrapMode="1">
					<Dimensions x="84" y="32" />
					<Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)LblTraitEnchantmentContainer" />
					<Anchor point="RIGHT" relativePoint="CENTER" relativeTo="$(parent)LblTraitEnchantmentContainer" offsetX="-24" />
					<OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside)</OnMouseUp>
					<OnMouseEnter>if self:GetParent().tooltipFunction then self:GetParent().tooltipFunction(self) end</OnMouseEnter>
					<OnMouseExit>if self:GetParent().tooltipExitFunction then self:GetParent().tooltipExitFunction() end</OnMouseExit>
					<OnReceiveDrag>if self:GetParent().receiveDragFunction then self:GetParent().receiveDragFunction() end</OnReceiveDrag>
				</Label>
				
				<Label name="$(parent)Enchantment" font="$(MEDIUM_FONT)|$(KB_16)|soft-shadow-thin" mouseEnabled="true" horizontalAlignment="0" verticalAlignment="CENTER" wrapMode="1">
					<Dimensions  y="32"  />
					<Anchor point="LEFT" relativePoint="CENTER" relativeTo="$(parent)LblTraitEnchantmentContainer" offsetX="-22"/>
					<Anchor point="RIGHT" relativePoint="RIGHT" relativeTo="$(parent)LblTraitEnchantmentContainer"  />
					<OnMouseUp>self:GetParent():GetHandler("OnMouseUp")(self, button, upInside)</OnMouseUp>
					<OnMouseEnter>if self:GetParent().tooltipFunction then self:GetParent().tooltipFunction(self) end</OnMouseEnter>
					<OnMouseExit>if self:GetParent().tooltipExitFunction then self:GetParent().tooltipExitFunction() end</OnMouseExit>
					<OnReceiveDrag>if self:GetParent().receiveDragFunction then self:GetParent().receiveDragFunction() end</OnReceiveDrag>
				</Label>
			</Controls>
		</Control>
		
		
		<Button name="CSPScppBtnPreset" virtual="true" horizontalAlignment="1" verticalAlignment="CENTER"  font="ZoFontGame">
			<Dimensions x="100" y="52" /> 
			<FontColors normalColor="E8DFAF" mouseOverColor="FFAA33" pressedColor="B96C00" />
			<OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>	
			<Controls>
				<Backdrop name="$(parent)BG" centerColor="080808" edgeColor="424242" >
					<Edge edgeSize="1" />
					<AnchorFill />
				</Backdrop>
			</Controls>
		</Button>
		<Button name="CSPSOptBtnPreset" font="ZoFontGame" inherits="ZO_DefaultButton" virtual="true">
			<Dimensions x="230" y="24" />
			<FontColors normalColor="E8DFAF" mouseOverColor="FFAA33"/>
		</Button>
		<Texture name="CSPSSkBarIconPres" color="242424" layer="1" mouseEnabled="true" virtual="true">
			<Dimensions x="28" y="28" />
			<OnReceiveDrag>CSPS.onHbIconReceive(self.i, self.j)</OnReceiveDrag>
			<OnMouseUp>
				WINDOW_MANAGER:SetMouseCursor(0)
				if button==2 then CSPS.hbSkillRemove(self.i, self.j) end
			</OnMouseUp>
			<OnDragStart>if button == 1 then WINDOW_MANAGER:SetMouseCursor(15)  CSPS.onHbIconDrag(self.i,self.j) end</OnDragStart>
		</Texture>	
		<Label name="CSPSCPPLABEL" font="ZoFontGame" wrapMode="ELLIPSIS" horizontalAlignment="LEFT" verticalAlignment="CENTER" virtual="true" />
		<Control name="CSPSCPHBSLOT" virtual="true">
			<Controls>
				<Texture name="$(parent)Circle" layer="2" mouseEnabled="true" textureFile="esoui/art/champion/actionbar/champion_bar_combat_selection.dds">
					<Dimensions x="24" y="24" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="3" />
					<OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>
				</Texture>	
				<Texture name="$(parent)Icon" layer="1" hidden="true" textureFile="esoui/art/champion/champion_icon_32.dds">
					<Dimensions x="24" y="24" />
					<Anchor point="CENTER" relativeTo="$(parent)Circle" relativePoint="CENTER" />			  
				</Texture>
				<Label font="$(MEDIUM_FONT)|$(KB_15)|soft-shadow-thin" wrapMode="1" horizontalAlignment="1" name="$(parent)Label" mouseEnabled="true"  >
					<Anchor point="LEFT" relativeTo="$(parent)Circle" relativePoint="RIGHT" offsetX="3" />		
					<Anchor point="RIGHT" relativeTo="$(parent)" relativePoint="RIGHT" offsetX="-3" constrains="X" />		
					<Dimensions y="24" />
				</Label>
			</Controls>
		</Control>
		
		<Control name="CSPSGroupBarTemplate" virtual="true">
			<Dimensions y="139" /> <!-- 5*25 + 5 + 3*3 -->
			<Controls>				
				<Control name="$(parent)Profiles" inherits="ZO_ComboBox" >
					<Dimensions y="25" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
					<Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" />
				</Control>				
				<Control name="$(parent)Slot1" inherits="CSPSCPHBSLOT">
					<OnInitialized>CSPS.groupSlotInit(self, 1)</OnInitialized>
					<Dimensions y="25" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)Profiles" relativePoint="BOTTOMLEFT" offsetY="5" />
					<Anchor point="TOPRIGHT" relativeTo="$(parent)Profiles" relativePoint="BOTTOMRIGHT" offsetY="5" />
				</Control>
				<Control name="$(parent)Slot2" inherits="CSPSCPHBSLOT">
					<OnInitialized>CSPS.groupSlotInit(self, 2)</OnInitialized>	
					<Dimensions y="25" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)Slot1" relativePoint="BOTTOMLEFT" offsetY="3" />
					<Anchor point="TOPRIGHT" relativeTo="$(parent)Slot1" relativePoint="BOTTOMRIGHT" offsetY="3" />
				</Control>
				<Control name="$(parent)Slot3" inherits="CSPSCPHBSLOT">
					<OnInitialized>CSPS.groupSlotInit(self, 3)</OnInitialized>
					<Dimensions y="25" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)Slot2" relativePoint="BOTTOMLEFT" offsetY="3" />
					<Anchor point="TOPRIGHT" relativeTo="$(parent)Slot2" relativePoint="BOTTOMRIGHT" offsetY="3" />
				</Control>
				<Control name="$(parent)Slot4" inherits="CSPSCPHBSLOT">
					<OnInitialized>CSPS.groupSlotInit(self, 4)</OnInitialized>
					<Dimensions y="25" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)Slot3" relativePoint="BOTTOMLEFT" offsetY="3" />
					<Anchor point="TOPRIGHT" relativeTo="$(parent)Slot3" relativePoint="BOTTOMRIGHT" offsetY="3" />
				</Control>
			
			</Controls>
		</Control>
		
		<Control name="CSPSCPKBLE" mouseEnabled="true" virtual="true">
			<Dimensions y="30" />
			<OnMouseEnter>CSPS.KBListRowMouseEnter(self)</OnMouseEnter>
			<OnMouseExit>CSPS.KBListRowMouseExit(self)</OnMouseExit>
			<OnMouseUp>CSPS.KBListRowMouseUp(self, button, upInside)</OnMouseUp>
			<Controls>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" color="A6D852" />
				<Label name="$(parent)Name" inherits="CSPSCPPLABEL">
					<Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)" offsetX="5"/>
					<Dimensions x="500" y="34" />
				</Label>
				<Button name="$(parent)Minus">
					<Dimensions y="29" x="29" />
					<Anchor point="CENTER" relativePoint="CENTER" relativeTo="$(parent)Name" constrains="Y"/>
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="CSPSWindowManageBarsListScrollBar" constrains="X" offsetX="-4"/>
					<Textures normal="ESOUI/art/buttons/minus_up.dds" pressed="ESOUI/art/buttons/minus_down.dds" mouseOver="ESOUI/art/buttons/minus_over.dds" />
					<OnMouseEnter>ZO_Tooltips_ShowTextTooltip(self, RIGHT, GetString(CSPS_Tooltiptext_DeleteBinding))</OnMouseEnter>
					<OnMouseExit>ZO_Tooltips_HideTextTooltip()</OnMouseExit>
				</Button>
			</Controls>
		</Control>
		
		<Control name="CSPSsubprofileLE" mouseEnabled="true" virtual="true">
			<Dimensions y="30" />
			<OnMouseEnter>CSPS.SubProfileListRowMouseEnter(self)</OnMouseEnter>
			<OnMouseExit>CSPS.SubProfileListRowMouseExit(self)</OnMouseExit>
			<OnMouseUp>CSPS.SubProfileListRowMouseUp(self, button, upInside, ctrl, alt, shift)</OnMouseUp>
			<Controls>
				<Texture name="$(parent)Icon">
					<Anchor point="LEFT" />
					<Dimensions x="26" y="26" />
				</Texture>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" color="A6D852"  />
